{
    "name": "pforret/leqm-php",
    "description": "PHP Package to do audio measurement/correction with leqm/goqm",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Pforret\\LeqmPhp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Peter Forret",
            "email": "peter@forret.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel/pint": "^1.25",
        "pestphp/pest": "*"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "pest",
        "lint": "pint"
    },
    "version": "0.2.1"
}
